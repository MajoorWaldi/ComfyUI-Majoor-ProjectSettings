{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error", "info", "debug"] }],
    "semi": ["error", "always"],
    "quotes": ["error", "double"]
  }
}
